public class Rectangle {
	private int l;
	private int L;
	
	Rectangle(){}
	
	Rectangle(int L, int l){
		this.l = l;
		this.L = L;
	}
	public void setl(int l) {
		this.l = l;
	}
	public void setL(int L) {
		this.L = L;
	}
	public int getl() {
		return l;
	}
	public int getL() {
		return L;
	}
	boolean testCote() {
		if(l>L) {
			return false;
		}else {
			return true;
		}
	}
	
	public String toString() {
		return "Longeur : "+L+"\n Largeur : "+l;
	}
}
